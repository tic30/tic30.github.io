{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:jest/recommended",
        "prettier"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "prettier",
        "jest"
    ],
    "rules": {
        "react/no-unescaped-entities": "off",
        "react/prop-types": "off"
    }
}